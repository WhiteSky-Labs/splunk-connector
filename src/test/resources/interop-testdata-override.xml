<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<connector name="Splunk">
    <properties>
        <connectivity-support>true</connectivity-support>
        <OAuth-authentication>false</OAuth-authentication>
        <metadata>static</metadata>
        <datasense-enabled>false</datasense-enabled>
        <transformer>false</transformer>
    </properties>
    <global-config>
        <config name="config">
            <attributes>
                <attribute prefix="splunk" javaType="java.lang.String" type="string" group="General" caption="Host"
                           use="required" name="host">localhost
                </attribute>
                <attribute prefix="splunk" javaType="int" type="integer" group="General" caption="Port" use="required"
                           name="port">8089
                </attribute>
                <attribute prefix="splunk" javaType="java.lang.String" type="string" group="Connection"
                           caption="Username" use="required" name="username">admin
                </attribute>
                <attribute prefix="splunk" javaType="java.lang.String" type="password" group="Connection"
                           caption="Password" use="required" name="password">admin
                </attribute>
            </attributes>
        </config>
    </global-config>
    <message-processors>
        <processor caption="Get applications" xmlName="get-applications" name="getApplications">
            <properties>
                <datamapper output="" input="List&lt;Map&gt;"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
        </processor>
        <processor caption="Get jobs" xmlName="get-jobs" name="getJobs">
            <properties>
                <datamapper output="" input="List&lt;Map&gt;"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
        </processor>
        <processor caption="Run blocking search" xmlName="run-blocking-search" name="runBlockingSearch">
            <properties>
                <datamapper output="Map" input="Map"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Search Query"
                           use="required" name="searchQuery"></attribute>
            </attributes>
        </processor>
        <processor caption="Run one shot search" xmlName="run-one-shot-search" name="runOneShotSearch">
            <properties>
                <datamapper output="Map&lt;String,String&gt;" input="List&lt;Map&gt;"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Search Query"
                           use="required" name="searchQuery"></attribute>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Earliest Time"
                           use="required" name="earliestTime"></attribute>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Latest Time"
                           use="required" name="latestTime"></attribute>
            </attributes>
        </processor>
        <processor caption="Get saved searches" xmlName="get-saved-searches" name="getSavedSearches">
            <properties>
                <datamapper output="" input="List&lt;Map&gt;"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
        </processor>
        <processor caption="Create saved search" xmlName="create-saved-search" name="createSavedSearch">
            <properties>
                <datamapper output="Map" input="Map"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Search Name"
                           use="required" name="searchName"></attribute>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Search Query"
                           use="required" name="searchQuery"></attribute>
            </attributes>
        </processor>
        <processor caption="View saved search properties" xmlName="view-saved-search-properties"
                   name="viewSavedSearchProperties">
            <properties>
                <datamapper output="" input="Set&lt;Entry&gt;"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Search Name"
                           use="required" name="searchName"></attribute>
            </attributes>
        </processor>
        <processor caption="Modify saved search properties" xmlName="modify-saved-search-properties"
                   name="modifySavedSearchProperties">
            <properties>
                <datamapper output="" input="Map"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Search Name"
                           use="required" name="searchName"></attribute>
                <attribute javaType="java.util.Map&lt;java.lang.String,java.lang.Object&gt;"
                           type="element-controller-map" group="General" caption="Search Properties" use="required"
                           name="search-properties"></attribute>
            </attributes>
        </processor>
        <processor caption="Get saved search history" xmlName="get-saved-search-history" name="getSavedSearchHistory">
            <properties>
                <datamapper output="" input="List&lt;Map&gt;"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
        </processor>
        <processor caption="Run saved search" xmlName="run-saved-search" name="runSavedSearch">
            <properties>
                <datamapper output="" input="List&lt;Map&gt;"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Search Name"
                           use="required" name="searchName"></attribute>
            </attributes>
        </processor>
        <processor caption="Run saved search with arguments" xmlName="run-saved-search-with-arguments"
                   name="runSavedSearchWithArguments">
            <properties>
                <datamapper output="Map" input="List&lt;Map&gt;"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Search Name"
                           use="required" name="searchName"></attribute>
            </attributes>
        </processor>
        <processor caption="Delete saved search" xmlName="delete-saved-search" name="deleteSavedSearch">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Search Name"
                           use="required" name="searchName"></attribute>
            </attributes>
        </processor>
        <processor caption="Get data model" xmlName="get-data-model" name="getDataModel">
            <properties>
                <datamapper output="" input="Map"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Data Model Name"
                           use="required" name="dataModelName"></attribute>
            </attributes>
        </processor>
        <processor caption="Get data models" xmlName="get-data-models" name="getDataModels">
            <properties>
                <datamapper output="" input="List&lt;Map&gt;"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
        </processor>
        <processor caption="Get inputs" xmlName="get-inputs" name="getInputs">
            <properties>
                <datamapper output="" input="List&lt;Map&gt;"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
        </processor>
        <processor caption="Create input" xmlName="create-input" name="createInput">
            <properties>
                <datamapper output="Map" input="Map"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Input Identifier"
                           use="required" name="inputIdentifier"></attribute>
                <attribute javaType="com.splunk.InputKind" type="string" group="General" caption="Kind Reference"
                           use="required" name="kind-ref"></attribute>
            </attributes>
        </processor>
        <processor caption="Modify input" xmlName="modify-input" name="modifyInput">
            <properties>
                <datamapper output="" input="Map"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Input Identifier"
                           use="required" name="inputIdentifier"></attribute>
                <attribute javaType="java.util.Map&lt;java.lang.String,java.lang.Object&gt;"
                           type="element-controller-map" group="General" caption="Input Args" use="required"
                           name="input-args"></attribute>
            </attributes>
        </processor>
        <processor caption="Get input" xmlName="get-input" name="getInput">
            <properties>
                <datamapper output="" input="Map"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Input Identifier"
                           use="required" name="inputIdentifier"></attribute>
            </attributes>
        </processor>
        <processor caption="Get indexes" xmlName="get-indexes" name="getIndexes">
            <properties>
                <datamapper output="SortDirection" input="List&lt;Map&gt;"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
        </processor>
        <processor caption="Create index" xmlName="create-index" name="createIndex">
            <properties>
                <datamapper output="Map" input="Map"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Index Name" use="required"
                           name="indexName"></attribute>
            </attributes>
        </processor>
        <processor caption="Modify index" xmlName="modify-index" name="modifyIndex">
            <properties>
                <datamapper output="" input="Map"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Index Name" use="required"
                           name="indexName"></attribute>
                <attribute javaType="java.util.Map&lt;java.lang.String,java.lang.Object&gt;"
                           type="element-controller-map" group="General" caption="Index Args" use="required"
                           name="index-args"></attribute>
            </attributes>
        </processor>
        <processor caption="Get index" xmlName="get-index" name="getIndex">
            <properties>
                <datamapper output="" input="Map"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Index Identifier"
                           use="required" name="indexIdentifier"></attribute>
            </attributes>
        </processor>
        <processor caption="Clean index" xmlName="clean-index" name="cleanIndex">
            <properties>
                <datamapper output="" input="Map"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Index Name" use="required"
                           name="indexName"></attribute>
            </attributes>
        </processor>
        <processor caption="Add data to index" xmlName="add-data-to-index" name="addDataToIndex">
            <properties>
                <datamapper output="Map" input="Map"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Index Name" use="required"
                           name="indexName"></attribute>
                <attribute javaType="java.lang.String" type="string" group="General" caption="String Data"
                           use="required" name="stringData"></attribute>
            </attributes>
        </processor>
        <processor caption="Add data to tcp input" xmlName="add-data-to-tcp-input" name="addDataToTcpInput">
            <properties>
                <datamapper output="" input="Boolean"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Port Number"
                           use="required" name="portNumber"></attribute>
                <attribute javaType="java.lang.String" type="string" group="General" caption="String Data"
                           use="required" name="stringData"></attribute>
            </attributes>
        </processor>
        <processor caption="Add data to udp input" xmlName="add-data-to-udp-input" name="addDataToUdpInput">
            <properties>
                <datamapper output="" input="Boolean"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Port Number"
                           use="required" name="portNumber"></attribute>
                <attribute javaType="java.lang.String" type="string" group="General" caption="String Data"
                           use="required" name="stringData"></attribute>
            </attributes>
        </processor>
        <processor caption="Remove input" xmlName="remove-input" name="removeInput">
            <properties>
                <datamapper output="" input="Boolean"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Input Identifier"
                           use="required" name="inputIdentifier"></attribute>
            </attributes>
        </processor>
        <processor caption="Remove index" xmlName="remove-index" name="removeIndex">
            <properties>
                <datamapper output="" input="Boolean"/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Index Name" use="required"
                           name="indexName"></attribute>
            </attributes>
        </processor>
    </message-processors>
</connector>
