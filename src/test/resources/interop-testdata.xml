<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<connector name="Splunk">
    <properties>
        <connectivity-support>true</connectivity-support>
        <OAuth-authentication>false</OAuth-authentication>
        <metadata>static</metadata>
        <datasense-enabled>true</datasense-enabled>
        <transformer>false</transformer>
    </properties>
    <global-config>
        <config name="config">
            <attributes>
                <attribute javaType="java.lang.String" type="name" group="Basic Settings" caption="Name" use="optional"
                           name="name">Splunk
                </attribute>
                <attribute prefix="splunk" javaType="java.lang.String" type="string" group="General" caption="Host"
                           use="required" name="host">localhost
                </attribute>
                <attribute prefix="splunk" javaType="java.lang.String" type="string" group="General" caption="Port"
                           use="required" name="port">8089
                </attribute>
                <attribute prefix="splunk" javaType="java.lang.String" type="string" group="Connection"
                           caption="Username" use="required" name="username">admin
                </attribute>
                <attribute prefix="splunk" javaType="java.lang.String" type="password" group="Connection"
                           caption="Password" use="required" name="password">admin
                </attribute>
            </attributes>
        </config>
    </global-config>
    <message-processors>
        <processor caption="Get applications" xmlName="get-applications" name="getApplications">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
        </processor>
        <processor caption="Get jobs" xmlName="get-jobs" name="getJobs">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
        </processor>
        <processor caption="Run blocking search" xmlName="run-blocking-search" name="runBlockingSearch">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Search Query"
                           use="required" name="searchQuery"></attribute>
                <attribute javaType="java.util.Map&lt;java.lang.String,java.lang.Object&gt;"
                           type="element-controller-map" group="General" caption="Search Args" use="optional"
                           name="search-args"></attribute>
            </attributes>
        </processor>
        <processor caption="Run one shot search" xmlName="run-one-shot-search" name="runOneShotSearch">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Search Query"
                           use="required" name="searchQuery"></attribute>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Earliest Time"
                           use="required" name="earliestTime"></attribute>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Latest Time"
                           use="required" name="latestTime"></attribute>
                <attribute javaType="java.util.Map&lt;java.lang.String,java.lang.String&gt;"
                           type="element-controller-map" group="General" caption="Args" use="optional"
                           name="args"></attribute>
            </attributes>
        </processor>
        <processor caption="Get saved searches" xmlName="get-saved-searches" name="getSavedSearches">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="App" use="optional"
                           name="app"></attribute>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Owner" use="optional"
                           name="owner"></attribute>
            </attributes>
        </processor>
        <processor caption="Create saved search" xmlName="create-saved-search" name="createSavedSearch">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Search Name"
                           use="required" name="searchName"></attribute>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Search Query"
                           use="required" name="searchQuery"></attribute>
                <attribute javaType="java.util.Map&lt;java.lang.String,java.lang.Object&gt;"
                           type="element-controller-map" group="General" caption="Search Args" use="optional"
                           name="search-args"></attribute>
            </attributes>
        </processor>
        <processor caption="View saved search properties" xmlName="view-saved-search-properties"
                   name="viewSavedSearchProperties">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Search Name"
                           use="required" name="searchName"></attribute>
                <attribute javaType="java.lang.String" type="string" group="General" caption="App" use="optional"
                           name="app"></attribute>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Owner" use="optional"
                           name="owner"></attribute>
            </attributes>
        </processor>
        <processor caption="Modify saved search properties" xmlName="modify-saved-search-properties"
                   name="modifySavedSearchProperties">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Search Name"
                           use="required" name="searchName"></attribute>
                <attribute javaType="java.util.Map&lt;java.lang.String,java.lang.Object&gt;"
                           type="element-controller-map" group="General" caption="Search Properties" use="required"
                           name="search-properties"></attribute>
            </attributes>
        </processor>
        <processor caption="Get saved search history" xmlName="get-saved-search-history" name="getSavedSearchHistory">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Search Name"
                           use="optional" name="searchName"></attribute>
                <attribute javaType="java.lang.String" type="string" group="General" caption="App" use="optional"
                           name="app"></attribute>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Owner" use="optional"
                           name="owner"></attribute>
            </attributes>
        </processor>
        <processor caption="Run saved search" xmlName="run-saved-search" name="runSavedSearch">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Search Name"
                           use="required" name="searchName"></attribute>
            </attributes>
        </processor>
        <processor caption="Run saved search with arguments" xmlName="run-saved-search-with-arguments"
                   name="runSavedSearchWithArguments">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Search Name"
                           use="required" name="searchName"></attribute>
                <attribute javaType="java.util.Map&lt;java.lang.String,java.lang.Object&gt;"
                           type="element-controller-map" group="General" caption="Custom Args" use="optional"
                           name="custom-args"></attribute>
                <attribute javaType="com.splunk.SavedSearchDispatchArgs" type="element-controller-map" group="General"
                           caption="Search Dispatch Args" use="optional" name="search-dispatch-args"></attribute>
            </attributes>
        </processor>
        <processor caption="Delete saved search" xmlName="delete-saved-search" name="deleteSavedSearch">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Search Name"
                           use="required" name="searchName"></attribute>
            </attributes>
        </processor>
        <processor caption="Get data model" xmlName="get-data-model" name="getDataModel">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Data Model Name"
                           use="required" name="dataModelName"></attribute>
            </attributes>
        </processor>
        <processor caption="Get data models" xmlName="get-data-models" name="getDataModels">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
        </processor>
        <processor caption="Get inputs" xmlName="get-inputs" name="getInputs">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
        </processor>
        <processor caption="Create input" xmlName="create-input" name="createInput">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Input Identifier"
                           use="required" name="inputIdentifier"></attribute>
                <attribute javaType="com.splunk.InputKind" type="string" group="General" caption="Kind Reference"
                           use="required" name="kind-ref"></attribute>
                <attribute javaType="java.util.Map&lt;java.lang.String,java.lang.Object&gt;"
                           type="element-controller-map" group="General" caption="Args" use="optional"
                           name="args"></attribute>
            </attributes>
        </processor>
        <processor caption="Modify input" xmlName="modify-input" name="modifyInput">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Input Identifier"
                           use="required" name="inputIdentifier"></attribute>
                <attribute javaType="java.util.Map&lt;java.lang.String,java.lang.Object&gt;"
                           type="element-controller-map" group="General" caption="Input Args" use="required"
                           name="input-args"></attribute>
            </attributes>
        </processor>
        <processor caption="Get input" xmlName="get-input" name="getInput">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Input Identifier"
                           use="required" name="inputIdentifier"></attribute>
            </attributes>
        </processor>
        <processor caption="Get indexes" xmlName="get-indexes" name="getIndexes">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Sort Key" use="optional"
                           name="sortKey"></attribute>
                <attribute javaType="com.splunk.CollectionArgs.SortDirection" type="enum" group="General"
                           caption="Sort Direction" use="optional" name="sortDirection"></attribute>
                <attribute javaType="java.util.Map&lt;java.lang.String,java.lang.Object&gt;"
                           type="element-controller-map" group="General" caption="Collection Parameters" use="optional"
                           name="collection-parameters"></attribute>
            </attributes>
        </processor>
        <processor caption="Create index" xmlName="create-index" name="createIndex">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Index Name" use="required"
                           name="indexName"></attribute>
                <attribute javaType="java.util.Map&lt;java.lang.String,java.lang.Object&gt;"
                           type="element-controller-map" group="General" caption="Args" use="optional"
                           name="args"></attribute>
            </attributes>
        </processor>
        <processor caption="Modify index" xmlName="modify-index" name="modifyIndex">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Index Name" use="required"
                           name="indexName"></attribute>
                <attribute javaType="java.util.Map&lt;java.lang.String,java.lang.Object&gt;"
                           type="element-controller-map" group="General" caption="Index Args" use="required"
                           name="index-args"></attribute>
            </attributes>
        </processor>
        <processor caption="Get index" xmlName="get-index" name="getIndex">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Index Identifier"
                           use="required" name="indexIdentifier"></attribute>
            </attributes>
        </processor>
        <processor caption="Clean index" xmlName="clean-index" name="cleanIndex">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Index Name" use="required"
                           name="indexName"></attribute>
                <attribute default="180" javaType="int" type="integer" group="General" caption="Max Seconds"
                           use="optional" name="maxSeconds"></attribute>
            </attributes>
        </processor>
        <processor caption="Add data to index" xmlName="add-data-to-index" name="addDataToIndex">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Index Name" use="required"
                           name="indexName"></attribute>
                <attribute javaType="java.lang.String" type="string" group="General" caption="String Data"
                           use="required" name="stringData"></attribute>
                <attribute javaType="java.util.Map&lt;java.lang.String,java.lang.Object&gt;"
                           type="element-controller-map" group="General" caption="Index Args" use="optional"
                           name="index-args"></attribute>
            </attributes>
        </processor>
        <processor caption="Add data to tcp input" xmlName="add-data-to-tcp-input" name="addDataToTcpInput">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Port Number"
                           use="required" name="portNumber"></attribute>
                <attribute javaType="java.lang.String" type="string" group="General" caption="String Data"
                           use="required" name="stringData"></attribute>
            </attributes>
        </processor>
        <processor caption="Add data to udp input" xmlName="add-data-to-udp-input" name="addDataToUdpInput">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Port Number"
                           use="required" name="portNumber"></attribute>
                <attribute javaType="java.lang.String" type="string" group="General" caption="String Data"
                           use="required" name="stringData"></attribute>
            </attributes>
        </processor>
        <processor caption="Remove input" xmlName="remove-input" name="removeInput">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Input Identifier"
                           use="required" name="inputIdentifier"></attribute>
            </attributes>
        </processor>
        <processor caption="Remove index" xmlName="remove-index" name="removeIndex">
            <properties>
                <datamapper output="" input=""/>
                <auto-paging>false</auto-paging>
                <query-support>false</query-support>
            </properties>
            <attributes>
                <attribute javaType="java.lang.String" type="string" group="General" caption="Index Name" use="required"
                           name="indexName"></attribute>
            </attributes>
        </processor>
    </message-processors>
    <inbound-endpoints>
        <endpoint caption="Run normal search" xmlName="run-normal-search" name="run-normal-search">
            <attributes>
                <attribute javaType="" type="" use="optional" name="key"></attribute>
                <attribute javaType="" type="" use="required" name="searchQuery"></attribute>
            </attributes>
            <childElements>
                <childElement use="optional" name="search-args">
                    <attributes>
                        <controller controllerType="" controlled="complex" caption="">
                            <attributes>
                                <attribute javaType="" type="" use="optional" name="key"></attribute>
                                <attribute javaType="" type="" use="optional" name="ref"></attribute>
                            </attributes>
                        </controller>
                        <controller controllerType="" controlled="useReference" caption="">
                            <attributes>
                                <attribute javaType="" type="" use="optional" name="ref"></attribute>
                            </attributes>
                        </controller>
                    </attributes>
                    <childElements>
                        <childElement use="optional" name="search-arg">
                            <attributes>
                                <controller controllerType="" controlled="complex" caption="">
                                    <attributes>
                                        <attribute javaType="" type="" use="optional" name="key"></attribute>
                                    </attributes>
                                </controller>
                                <controller controllerType="" controlled="useReference" caption="">
                                    <attributes>
                                        <attribute javaType="" type="" use="optional" name="ref"></attribute>
                                    </attributes>
                                </controller>
                            </attributes>
                            <childElements/>
                        </childElement>
                    </childElements>
                </childElement>
            </childElements>
        </endpoint>
        <endpoint caption="Run real time search" xmlName="run-real-time-search" name="run-real-time-search">
            <attributes>
                <attribute javaType="" type="" use="required" name="searchQuery"></attribute>
                <attribute default="rt" javaType="" type="" use="optional" name="earliestTime"></attribute>
                <attribute default="rt" javaType="" type="" use="optional" name="latestTime"></attribute>
                <attribute default="300" javaType="" type="" use="optional" name="statusBuckets"></attribute>
                <attribute default="100" javaType="" type="" use="optional" name="previewCount"></attribute>
            </attributes>
            <childElements/>
        </endpoint>
        <endpoint caption="Run export search" xmlName="run-export-search" name="run-export-search">
            <attributes>
                <attribute javaType="" type="" use="required" name="searchQuery"></attribute>
                <attribute default="-1h" javaType="" type="" use="optional" name="earliestTime"></attribute>
                <attribute default="now" javaType="" type="" use="optional" name="latestTime"></attribute>
            </attributes>
            <childElements/>
        </endpoint>
    </inbound-endpoints>
</connector>
